# Dockerator .gitignore

# =================================================================
# Docker Compose - Dynamic file (gets modified by scripts)
# =================================================================
docker-compose.yml

# =================================================================
# Apps - Individual projects have their own repos
# =================================================================
apps/*
!apps/.gitkeep

# =================================================================
# Environment files
# =================================================================
.env.local
.env*.local

# =================================================================
# OS Files
# =================================================================
.DS_Store
Thumbs.db

# =================================================================
# IDE
# =================================================================
.vscode/
.idea/
*.swp
*.swo

# =================================================================
# Logs
# =================================================================
*.log
logs/

# =================================================================
# Backup files (created by scripts)
# =================================================================
*.bak
docker-compose.yml.bak
